	<view name="rpt_new_release_detail" table="tblCSW_ReportNewReleaseDetail" key="id">
		<fields>
		<field name="id" dt="int" sort="1" />
		<field name="dtRelease" dt="datetime" sort="1" format="d" />			
		<field name="sTitle" dt="string" sort="1" >
			<filter type="text" op="like" >
				<param name="maxlength" value="260" />
				<param name="size" value="15" />
			</filter>
		</field>
		<field name="sArtist" dt="string" sort="1" >
			<filter type="text" op="like" >
				<param name="maxlength" value="100" />
				<param name="size" value="15" />
			</filter>
		</field>
		<field desc="idGenre" name="genre" dt="string" sort="1">
			<filter type="lookupdb" op="like">
				<param name="sql" value="select * from ( select G.sDescription, M.sName + ' - ' + G.sDescription as sDisplay from tblMMD_MetaDataGenre G join tblMMD_MetaDataMediaTypeGenre MG ON G.idGenre = MG.idGenre JOIN tblMMD_MetaDataMediaType M ON MG.idMediaType = M.idMediaType where M.bUseInDevice = 1 and G.bUseInDevice = 1 union select sGamePlatform, 'GAME - ' + sGamePlatform from tblMMD_MetaDataGamePlatform ) X order by sDisplay" />
			</filter>
			<display type="lookupdb" op="like">
				<param name="sql" value="select sDescription, sDescription from tblMMD_MetaDataGenre union select sGamePlatform, sGamePlatform from tblMMD_MetaDataGamePlatform"/>
			</display>
		</field>		

		<field name="idProduct" dt="int" sort="0" />
		<field name="sUPC" dt="string" sort="0" />
		<field name="cViews" dt="int" sort="1" >
            <filter type="text" op="eq">
                <param name="maxlength" value="10"/>
                <param name="size" value="4"/>
            </filter>			
		</field>			
		<field name="cSampled" dt="int" sort="1" >
            <filter type="text" op="eq">
                <param name="maxlength" value="10"/>
                <param name="size" value="4"/>
            </filter>			
		</field>			
		<field name="cScans" dt="int" sort="1" >
            <filter type="text" op="eq">
                <param name="maxlength" value="10"/>
                <param name="size" value="4"/>
            </filter>			
		</field>			
		<field name="cSelects" dt="int" sort="1" >
            <filter type="text" op="eq">
                <param name="maxlength" value="10"/>
                <param name="size" value="4"/>
            </filter>			
		</field>			
		</fields>		
	</view>
