    <view name="event_package_list" table="tblMMD_Package" key="idPackage" >
		<fields>
			<field name="idPackage" dt="int" sort="0" />
			<field name="idSourceLevel" dt="int" sort="0">
				<filter type="lookupenum" op="eq">
					<param name="type" value="MMD.Central.CentralLib.XmlObjects.MMDPackageLevel"/>
				</filter>
				<display type="lookupenum" op="eq">
					<param name="type" value="MMD.Central.CentralLib.XmlObjects.MMDPackageLevel"/>
				</display>
			</field>
			<field name="idDestLevel" dt="int" sort="0">
				<filter type="lookupenum" op="eq">
					<param name="type" value="MMD.Central.CentralLib.XmlObjects.MMDPackageLevel"/>
				</filter>
				<display type="lookupenum" op="eq">
					<param name="type" value="MMD.Central.CentralLib.XmlObjects.MMDPackageLevel"/>
				</display>
			</field>
			<field name="idSendEvent" dt="int" sort="0">
				<filter type="lookupdb" op="eq">
					<param name="sql" value="select idEventSchedule, sDesc from tblMMD_EventSchedule"/>
				</filter>
				<display type="lookupdb" op="eq">
					<param name="sql" value="select idEventSchedule, sDesc from tblMMD_EventSchedule"/>
				</display>
			</field>
			<field name="idRecvEvent" dt="int" sort="0">
				<filter type="lookupdb" op="eq">
					<param name="sql" value="select idEventSchedule, sDesc from tblMMD_EventSchedule"/>
				</filter>
				<display type="lookupdb" op="eq">
					<param name="sql" value="select idEventSchedule, sDesc from tblMMD_EventSchedule"/>
				</display>
			</field>
			<field name="sDesc" dt="string" sort="0" />
			<field name="sTransportSubdir" dt="string" sort="0" />
		</fields>
    </view>
    <view name="event_schedule" table="tblMMD_EventSchedule" key="idEventSchedule" >
		<fields>
			<field name="idEventSchedule" dt="int" sort="0" />
			<field name="byType" dt="int" sort="0" />
			<field name="iPeriodMinutes" dt="int" sort="0" />
			<field name="dtTimeOfDay" dt="int" sort="0" />
			<field name="bfDaysOfWeek" dt="int" sort="0" />
			<field name="dtRunOnce" dt="int" sort="0" />
			<field name="bRunNow" dt="int" sort="0" />
			<field name="sDesc" dt="string" sort="1" >
				<filter type="text" op="like" >
					<param name="maxlength" value="100" />
					<param name="size" value="10" />
				</filter>
			</field>
			<field name="iPriority" dt="int" sort="1" >
				<filter type="text" op="eq">
					<param name="maxlength" value="10"/>
					<param name="size" value="4"/>
				</filter>					
			</field>
			<field name="byStatus" dt="int" sort="0">
				<filter type="lookupenum" op="eq">
					<param name="type" value="MMD.Central.CentralLib.XmlObjects.EventRunStatus" />
				</filter>
				<display type="lookupenum">
					<param name="type" value="MMD.Central.CentralLib.XmlObjects.EventRunStatus" />
				</display>
			</field>
			<field name="dtLastRun" dt="datetime" sort="0" />
		</fields>
    </view>
