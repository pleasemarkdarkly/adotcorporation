                <filter type="text" op="eq">
                    <param name="maxlength" value="10"/>
                    <param name="size" value="3"/>
                </filter>			
			</field>			
			<field name="cSelects" dt="int" sort="1" >
                <filter type="text" op="eq">
                    <param name="maxlength" value="10"/>
                    <param name="size" value="3"/>
                </filter>			
			</field>			
			<field name="cTotal" dt="int" sort="1" >
                <filter type="text" op="eq">
                    <param name="maxlength" value="10"/>
                    <param name="size" value="3"/>
                </filter>			

			</field>
		</fields>
	</view>
