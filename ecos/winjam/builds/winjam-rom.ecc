    # option CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_ICTRL
    #     DefaultValue:  CYGSEM_HAL_VIRTUAL_VECTOR_INIT_WHOLE_TABLE  || CYGSEM_HAL_VIRTUAL_VECTOR_CLAIM_DEFAULT 
